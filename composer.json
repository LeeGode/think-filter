{
    "name": "leegode/think-filter",
    "description": "think-filter",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Leegode\\ThinkFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leegode\\ThinkFilter\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Leegode",
            "email": "liwei_1991@126.com"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "topthink/framework": "^5.1.10 || ^6.0.0",
        "topthink/think-helper": "^3.1.1"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "extra": {
        "think": {
            "services": [
                "Leegode\\ThinkFilter\\ThinkFilterService"
            ],
            "config":{
                "filter": "src/config.php"
            }
        }
    }
}
